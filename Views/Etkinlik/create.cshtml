@model gsu_math.ViewModels.EtkinlikCreateViewModel
<form enctype="multipart/form-data" asp-action="Create" id="my-form">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="Baslik" class="control-label">Başlık:</label>
        <input class="form-control" asp-for="Baslik">
    </div>
    <div class="form-group">
        <label asp-for="foto" class="control-label"></label>
        <input class="form-control" asp-for="foto" type="file" accept="image/*">
    </div>
    <div class="form-group">
        <label asp-for="startDate" class="control-label"></label>
        <input class="form-control" asp-for="startDate">
    </div>
    <div class="form-group">
        <label asp-for="endDate" class="control-label"></label>
        <input class="form-control" asp-for="endDate">
    </div>
    <div class="form-group">
        <label asp-for="Metin" class="control-label"></label>
        <input class="form-control" rows=4 asp-for="Metin">
    </div>
    <input type="submit">
</form>