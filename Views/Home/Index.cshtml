@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Bienvenue à la Maison</h1>
    <h3>Üyeler</h3>
    <img src="~/images/harry-potter-lord-voldemort.jpeg" alt="" class="w-100">

    @foreach (var item in ViewBag.users)
    {
        <p>@item.Username</p>
        <p>@item.Email</p>
        <p>@item.AdSoyad</p>
        <p>@item.Password</p>
        <p>@item.AtCreated</p>
        <p>@item.Status</p>
        <p>@item.Yetki @@ yetkiler   </p>
    }
</div>
